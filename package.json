{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/test": "^3.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/fabric": "^2.0.5",
    "prettier": "^2.0.1",
    "stylelint": "^13.0.0",
    "lint-staged": "^10.0.7",
    "husky": "^4.0.7"
  },
  "engines": { "node": ">=10.13.0" }
}
